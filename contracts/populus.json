          },
          "TestContracts": {
            "$ref": "contracts.backends.TestContracts"
          }
        }
      },
      "web3": {
        "eth": {
          "default_account": "0xbB5AEb01acF5b75bc36eC01f5137Dd2728FbE983"
        },
        "provider": {
          "class": "web3.providers.rpc.HTTPProvider",
            "settings": {
            "endpoint_uri": "http://127.0.0.1:8545",
            "request_kwargs": {
                "timeout": 180
            }
          }
        }
      }
    },
